  "emmet.preferences": {},
  // "editor.formatOnSave": true,
  // "editor.defaultFormatter": "Prettier",
  "leek-fund.immersiveBackground": true,
  "leek-fund.showEarnings": 0,
  "editor.fontFamily": "Fira Code", //后边的引号中写上要设置的字体类型，个人比较喜欢Fira Code
  "editor.fontLigatures": false, //这个控制是否启用字体连字，true启用，false不启用，这里选择启用
  "editor.fontSize": 14, //设置字体大小，这个不多说都明白
  "editor.fontWeight": "normal", //这个设置字体粗细，可选normal,bold,"100"~"900"等，选择合适的就行
  "prettier.arrowParens": "avoid",
  "prettier.singleQuote": true,
  "eslint.workingDirectories": [],
  "files.autoSave": "onWindowChange",
  "editor.quickSuggestionsDelay": 2,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "files.autoSaveDelay": 500,
  "files.associations": {
    "*.wpy": "vue",
    "*.vue": "vue",
    "*.tpl": "html"
  },
  "emmet.syntaxProfiles": {
    "vue-html": "html",
    "vue": "html"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "html",
    "jsx",
    {
      "language": "html",
      "autoFix": true
    },
    {
      "language": "vue",
      "autoFix": true
    }
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.enable": true,
  "eslint.format.enable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.options": {}, //使用单引号而不是双引号
  "prettier.jsxBracketSameLine": true,
  "prettier.arrowParens": "avoid",
  "prettier.singleQuote": true,
  "vetur.validation.interpolation": false, //保存时自动格式化
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.jsxSingleQuote": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true
  },
  "editor.codeActionsOnSaveTimeout": 5000,

  "vetur.format.defaultFormatterOptions": {
    "prettyhtml": {
      "printWidth": 100,
      "singleQuote": true,
      "wrapAttributes": false,
      "sortAttributes": false
    },
    "js-beautify-html": {
      "wrap_attributes": "force-expand-multiline"
    },

    "prettier": {
      // Prettier option here
      "htmlWhitespaceSensitivity": "ignore",
      "tabWidth": 2, // 会忽略vetur的tabSize配置
      "useTabs": false, // 是否利用tab替代空格
      "semi": false, // 句尾是否加;
      "singleQuote": true, // 使用单引号而不
      "arrowParens": "avoid" // allow paren-less arrow functions 箭头函数的参数使用圆括号
    },
    "js-beautify-html": {
      "wrap_attributes": "force-expand-multiline", //   "wrap_attributes": "auto", // " // html 标签属性 换行设置[auto|force|force-aligned|force-expand-multiline] [“auto”]
      "wrap_line_length": 0, // 设置多个字符后换行 0 表示忽略
      "end_with_newline": false, // 在文件结尾添加新行
      "semi": false //   "wrap_attributes": "aligned-multiple"
    }
  },

  // "vetur.format.defaultFormatter.html": "js-beautify-html",
  "vetur.experimental.templateInterpolationService": true,
  "vetur.format.defaultFormatter.js": "prettier-eslint",
